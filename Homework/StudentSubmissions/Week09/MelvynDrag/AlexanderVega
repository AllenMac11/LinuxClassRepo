# Problem 1 )

	curl -X  GET -s https://jsonplaceholder.typicode.com/users/1/posts | grep "userId" | sort -u | wc -l


# problem 2 ) 

	curl -X POST --header "Content-Type: application/json" -d'{"userId": 10337, "id": 0, "title": "hello scrub", "body": "this is a body"}' jsonplaceholder.typicode.com/posts


	curl -X POST --header "Content-Type: application/json" -d'{"userId": 10338, "id": 0, "title": "hello scrublet", "body": "this is a body"}' jsonplaceholder.typicode.com/posts


	curl -X POST --header "Content-Type: application/json" -d'{"userId": 10339, "id": 0, "title": "hello scrublet 1", "body": "this is a body"}' jsonplaceholder.typicode.com/posts

	curl -X POST --header "Content-Type: application/json" -d'{"userId": 10440, "id": 0, "title": "hello scrublet 2", "body": "this is a body"}' jsonplaceholder.typicode.com/posts


# problem 3 )

	curl -X POST --verbose --header "Content-Type: application/json" -d'{"userId": 10440, "id": 0, "title": "hello scrublet 2", "body": "this is a body"}' jsonplaceholder.typicode.com/posts

	charset = utf-8; 8 bit unicode


# problem 4 ) 


	curl -X POST -H "Content-Type: application/json" @~/Desktop/data.json jsonplaceholder.typicode.com/posts


